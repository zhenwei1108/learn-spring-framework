<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">


	<context:property-placeholder location="mine.properties" />

	<bean class="com.zhenwei.spring.learing.beans.User" name="user">
		<property name="userName" value="${myName}" />
		<property name="password" value="haha" />
		<property name="likes" >
			<list>
				<value>xizao</value>
				<value>shuijiao</value>

			</list>

		</property>
	</bean>


	<context:component-scan base-package="com.zhenwei.spring.*" />

</beans>